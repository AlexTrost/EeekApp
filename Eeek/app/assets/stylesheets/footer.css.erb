.footer {
	border: 3px orange solid;
	width: 100%;
	margin-bottom: 0;
	text-align: center;
	font-family: 'Bree Serif';
}

.footer_header {
	border: 3px red solid;
	font-size: 28px;
}

.footer_nav {
	border: 3px green solid;
}

.footer_ul {
	list-style-type: none;
}

.footer_ul li {
	display: inline;
}

.footer_slash {
	margin: 0 2% 0 2%;
	color: gray;
}

.footer_signature {
	border: 3px blue solid;
	font-size: 12px;
}