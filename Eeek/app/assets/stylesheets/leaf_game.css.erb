.game_console {
	border: 3px orange solid;
	height: 5em;
	width: 10em;
	position: absolute;
	left: 82%;
	top: 80%;
}

.game_console ul {
	list-style-type: none;
}

.game_console ul li {
	border: 3px blue solid;
	height: 5em;
	width: 40%;
	display: inline-block;
}

li.score {
	color: orange;
}

li.game_start {
	background-image: url(<%= asset_path 'leaf3.png' %>);
	background-repeat: no-repeat;
	background-size: 100%; 
	background-color: orange;
}

.click_leaf {
	height: 3em;
	width: 3em;
	border: 3px blue solid;
	background-image: url(<%= asset_path 'leaf3.png' %>);
	background-repeat: no-repeat;
	background-size: 100%; 
	background-color: orange;
	position: absolute;
}